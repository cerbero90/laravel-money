{
  "name": "cknow/laravel-money",
  "description": "Laravel Money",
  "keywords": [
    "laravel",
    "money",
    "currency"
  ],
  "homepage": "https://github.com/cknow/laravel-money",
  "license": "MIT",
  "authors": [
    {
      "name": "Ricardo Gobbo de Souza",
      "email": "ricardogobbosouza@yahoo.com.br",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "ricardo@clicknow.com.br",
    "issues": "https://github.com/cknow/laravel-money/issues",
    "source": "https://github.com/cknow/laravel-money"
  },
  "require": {
    "php": "^7.2.5",
    "ext-json": "*",
    "ext-intl": "*",
    "illuminate/contracts": "^7.0",
    "illuminate/support": "^7.0",
    "illuminate/view": "^7.0",
    "moneyphp/money": "^3.3"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16",
    "graham-campbell/testbench": "^5.4",
    "illuminate/filesystem": "^7.0",
    "mockery/mockery": "^1.3",
    "nikic/php-parser": "^4.4",
    "php-parallel-lint/php-parallel-lint": "^1.2",
    "phpmd/phpmd": "^2.8",
    "phpro/grumphp": "^0.18",
    "phpstan/phpstan": "^0.12",
    "phpunit/phpunit": "^8.5|^9.0",
    "povils/phpmnd": "^2.2",
    "sensiolabs/security-checker": "^6.0",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "Cknow\\Money\\": "src/"
    }
  },
  "autoload-dev": {
    "files": [
      "tests/Database/Migrations/2020_04_30_000000_create_users_table.php"
    ],
    "psr-4": {
      "Cknow\\Money\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": [
      "grumphp run"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Cknow\\Money\\MoneyServiceProvider"
      ]
    }
  }
}
